<body class="gray-bg">
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-sm-12">

                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>Select</h5>
                    </div>

                    <form action="~/Staffs/PlotChart" method="get">
                        <div class="ibox-content">
                            <div class="form-group">
                                <label class="font-noraml">The chart type</label>
                                <div class="input-group">
                                    <select name="type" data-placeholder="Column1" class="chosen-select" style="width:350px;" tabindex="2">
                                        @if (ViewBag.plotType == "Column")
                                        {
                                            <option value="Column">Column</option>
                                            <option value="Pie" hassubinfo="true">Pie</option>
                                            <option value="Line" hassubinfo="true">Line</option>
                                        }
                                        @if (ViewBag.plotType == "Pie")
                                        {
                                            <option value="Pie">Pie</option>
                                            <option value="Column" hassubinfo="true">Column</option>
                                            <option value="Line" hassubinfo="true">Line</option>
                                        }
                                        @if (ViewBag.plotType == "Line")
                                        {
                                            <option value="Line">Line</option>
                                            <option value="Column" hassubinfo="true">Column</option>
                                            <option value="Pie" hassubinfo="true">Pie</option>
                                        }
                                    </select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="font-noraml">The chart dimension</label>
                                <div class="input-group">
                                    <select name="dim" data-placeholder="2D1" class="chosen-select" style="width:350px;" tabindex="2">
                                        @if (ViewBag.dim == "2D")
                                        {
                                            <option value="2D">2D</option>
                                            <option value="3D" hassubinfo="true">3D</option>
                                        }
                                        @if (ViewBag.dim == "3D")
                                        {
                                            <option value="3D">3D</option>
                                            <option value="2D" hassubinfo="true">2D</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <p>
                                <button type="submit" class="btn btn-outline btn-default">View</button>
                            </p>

                        </div>
                    </form>
                </div>
            </div>
        </div>

        @if (ViewBag.plotType == "Column")
        {
            <div class="row">
                <div class="col-sm-6">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Column</h5>
                        </div>
                        <div class="ibox-content">
                            <p>
                                x: type y: number<br />
                                @{int i = 0;}
                                @foreach (var item in ViewBag.permitType)
                                {
                                    i++;
                                    @i 
                                    <span>: </span>
                                    @item.Type;
                                    <span>&nbsp;&nbsp;</span>
                                }
                            </p>
                        </div>
                        <div class="ibox-content">
                            <div class="flot-chart">
                                <div class="flot-chart-content" id="flot-bar-chart"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
        @if (ViewBag.plotType == "Line")
        {
            <div class="row">
                <div class="col-sm-6">
                    <div class="ibox float-e-margins">
