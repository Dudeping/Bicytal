                                <div class="flot-chart-content" id="flot-pie-chart"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }

        @foreach (var item in @ViewBag.permitType)
        {
            <div class="permitType" hidden="hidden">@item.Type</div>
            <div class="permitNum" hidden="hidden">@ViewBag.PurNum[item.Type]</div>
